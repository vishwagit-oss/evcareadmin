# EVCare Admin - Environment Variables
# Copy to .env.local and fill in your values

DATABASE_URL=postgresql://user:password@host:5432/database
NEXT_PUBLIC_COGNITO_USER_POOL_ID=your_user_pool_id
NEXT_PUBLIC_COGNITO_CLIENT_ID=your_public_client_id
AWS_REGION=us-east-2

# Phase 4 - AWS Enhancements (optional for local dev)
EVCARE_S3_BUCKET=evcare-reports
EVCARE_ALERT_FROM=noreply@evcare.local
EVCARE_ALERT_EMAIL=admin@example.com
EVCARE_SECRET_NAME=evcare/production/config
EVCARE_LOG_GROUP=/evcare/admin
EVCARE_LOG_STREAM=api
